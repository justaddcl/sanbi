{
  "name": "sanbi",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:start": "open -a Docker && sleep 1 && ./start-database.sh && pnpm db:studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:push:prod": "NODE_ENV=production drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "dotenvx run -- npx tsx ./src/server/db/seed.ts",
    "db:seed:prod": "NODE_ENV=production dotenvx run -- npx tsx ./src/server/db/seed.ts",
    "db:clear": "dotenvx run -- npx tsx ./src/server/db/clear.ts",
    "db:clear:prod": "NODE_ENV=production dotenvx run -- npx tsx ./src/server/db/clear.ts",
    "db:update": "dotenvx run -- npx tsx ./src/server/db/clear.ts && drizzle-kit push && dotenvx run -- npx tsx ./src/server/db/seed.ts",
    "db:update:prod": "NODE_ENV=production dotenvx run -- npx tsx ./src/server/db/clear.ts && NODE_ENV=production drizzle-kit push && NODE_ENV=production dotenvx run -- npx tsx ./src/server/db/seed.ts",
    "dev": "next dev",
    "lint": "next lint && npx tsc --noEmit --project tsconfig.json",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch"
  },
